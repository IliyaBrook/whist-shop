{
  "name": "whist-shop",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "install-client": "cd whist-shop-client && npm install",
    "start-prod": "set NODE_ENV=production && nodemon app.js",
    "build": "cd whist-shop-client && npm run build",
    "heroku-postbuild": "npm run install-client && npm run build",
    "server": "nodemon app.js",
    "client": "cd ./whist-shop-client && npm start",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run start --prefix whist-shop-client\""
  },
  "engines": {
    "node": "16.6.1"
  },
  "author": "Iliya brook",
  "license": "ISC",
  "dependencies": {
    "@types/mongoose": "^5.11.97",
    "body-parser": "^1.19.1",
    "concurrently": "^6.2.0",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.1",
    "mongodb": "^3.6.9",
    "mongoose": "^6.1.2",
    "node": "^14.17.1",
    "nodemon": "^2.0.15"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "webpack": "^5.62.1",
    "webpack-cli": "^4.8.0"
  }
}
